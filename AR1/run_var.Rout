> ar1 <- function(N, phi, omega, start)
+ {
+ 	out <- numeric(length = N)
+ 	out[1] <- start
+ 	eps <- rnorm(N, sd = sqrt(omega))
+ 	for(t in 2:N)
+ 	{
+ 		out[t] <- phi*out[t-1] + eps[t]
+ 	}
+ 	return(out)
+ }
